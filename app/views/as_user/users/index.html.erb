<h1>users</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Email</th>
    <th>Name</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= "***" || user.email %></td>
    <td><%= user.name %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
